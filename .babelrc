{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": ["last 2 versions", "ie >= 10"],
        "useBuiltIns": "entry",
        "corejs": "core-js@2"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
